ont:Synesis rdf:type owl:NamedIndividual ,
                     ont:RhetoricalFigure ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty ont:isLinguisticGroup ;
                       owl:someValuesFrom ont:LinguisticGroup
