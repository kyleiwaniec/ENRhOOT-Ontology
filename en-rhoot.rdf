###  https://github.com/kyleiwaniec/ENRhOOT-Ontology/en-rhoot.owl#Accismus
ont:Accismus rdf:type owl:NamedIndividual ,
                      ont:RhetoricalFigure ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty ont:isLinguisticGroup ;
                        owl:someValuesFrom ont:LinguisticGroup
                      ] ;
             ont:isInObject ont:Phraseobject ,
                            ont:Sentence ,
                            ont:Wordobject ;
             ont:isLinguisticGroup ont:Semantic ;
             ont:isReplacedByAnotherElementOfOppositeMeaning ont:Word_Element ;
             ont:isRhetoricalGroup ont:Trope ;
             ont:spans ont:Sentence ;
             ont:Name "Accismus"^^xsd:string ;
             ont:isExample "“Das Geschenk kann ich wirklich nicht annehmen!”"^^xsd:string ,
                           "“Das hat er nicht wirklich gesagt.”"^^xsd:string ;
             rdfs:comment "A form of irony in which a person feigns indifference to or pretends to refuse something he or she desires."^^xsd:string ;
             rdfs:label "Accismus"@en ,
                        "Akizam"@sr ,
                        "Akkismós"@el ,
                        "Akzismus"@de ;
             rdfs:seeAlso "Accism"@en .


###  https://github.com/kyleiwaniec/ENRhOOT-Ontology/en-rhoot.owl#Accumulation
ont:Accumulation rdf:type owl:NamedIndividual ,
                          ont:RhetoricalFigure ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ont:isLinguisticGroup ;
                            owl:someValuesFrom ont:LinguisticGroup
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ont:isRhetoricalGroup ;
                            owl:someValuesFrom ont:RhetoricalGroup
                          ] ;
                 ont:AnotherElementOfGreaterImportanceIsAdded ont:Phrase_Element ;
                 ont:isInObject ont:Phraseobject ;
                 ont:isInPosition ont:Whole-Succession ;
                 ont:isLinguisticGroup ont:Semantic ;
                 ont:isRhetoricalGroup ont:FigureOfSpeech ;
                 ont:spans ont:Sentence ,
                           ont:Verse ;
                 ont:Name "Akkumulatio"^^xsd:string ;
                 ont:isExample "\"Ihre Anzahl ist Legion: Staatsraete und Regierungsraete, Landraete und Kreisraete, Geistliche Raete und Schulraete, Finanzraete und Forstraete usw.\" (Georg Buechner: Der hessische Landbote)"^^xsd:string ,
                               "\"Mord! Zeter! Jammer! Angst! Creutz! Marter! Wuerme! Plagen!\" (Andreas Gryphius: Die Hoelle)"^^xsd:string ,
                               "\"ein stiller, hagerer, duenner, laenglicher Mann\" (Adelbert von Chamisso: Peter Schlehmihls wunderbare Geschichte)"^^xsd:string ;
                 rdfs:comment "Accumulation is a figure of speech in rhetoric that creates a list or gathers scattered ideas in a way that builds up, emphasizes, or summarizes the main point. Accumulation is an example of addition in rhetoric, using a “more the merrier” approach to illustrating the theme of a passage."^^xsd:string ;
                 rdfs:label "Accumulation"@en ,
                            "Akkumulation"@de ,
                            "Pojacanje"@sr .


###  https://github.com/kyleiwaniec/ENRhOOT-Ontology/en-rhoot.owl#Acrostic
ont:Acrostic rdf:type owl:NamedIndividual ,
