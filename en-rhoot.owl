    <!-- https://ramonakuehn.de/grhoot.owl#Akrostichon -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl#Akrostichon">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl#RhetorischeFigur"/>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ramonakuehn.de/grhoot.owl#istLinguistischeGruppe"/>
