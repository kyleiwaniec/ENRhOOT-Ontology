        <ont:istInPosition rdf:resource="https://ramonakuehn.de/grhoot.owl#GANZE-NACHFOLGEND"/>
        <ont:istLinguistischeGruppe rdf:resource="https://ramonakuehn.de/grhoot.owl#SEMANTIC"/>
        <ont:istRhetorischeGruppe rdf:resource="https://ramonakuehn.de/grhoot.owl#BETONUNGSFIGUR"/>
