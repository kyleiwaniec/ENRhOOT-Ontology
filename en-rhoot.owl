    <!-- https://ramonakuehn.de/grhoot.owl#Parabel -->

    <owl:NamedIndividual rdf:about="https://ramonakuehn.de/grhoot.owl#Parabel">
        <rdf:type rdf:resource="https://ramonakuehn.de/grhoot.owl#RhetorischeFigur"/>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ramonakuehn.de/grhoot.owl#istLinguistischeGruppe"/>
                <owl:someValuesFrom rdf:resource="https://ramonakuehn.de/grhoot.owl#LinguistischeGruppe"/>
            </owl:Restriction>
        </rdf:type>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ramonakuehn.de/grhoot.owl#istRhetorischeGruppe"/>
